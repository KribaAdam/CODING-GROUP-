def bfs(graph, start, visited) : #تعريف الدالة : مجموعة تعريف،نقطة بداية، و نقطة نهاية  
    visited =set() # مجموعة تحفظ النقاط التي تمت زيارتها 
    queue = ['A'] # قائمة تحتفظ بالنقاط التي تمت زيارتها نبدأ بنقطة البداية
    while queue : # عندما تكون القائمة غير فارغة 
        node = queue.pop(0) # نخرج النقطة الأولى من القائمة ( النقطة التي تمت زيارتها فعلا )
        if node not in visited : # إذا كانت النقطة غير مزارة
            print(node,end  = ' ') # نطبع النقطة
            visited.add(node) # إضافة النقطة إلة قائمة الزيارة
            for neighbor in graph[node] : # من أجل النقاط الموجودة في نفس مستوى النقطة
                if neighbor not in visited : # إذا كانت النقطة المجاورة غير مزارة
                    queue.append(neighbor) # اضافة النقطة إلى قائمة الإنتظار
graph = {  # تعريف الرسم البياني 
    'A': ['B', 'C'],
    'B': ['D', 'E'],
    'C': ['F'],
    'D': [],
    'E': [],
    'F': []
} 
bfs(graph, 'A', 'F') # إستدعاء الدالة
 
 
